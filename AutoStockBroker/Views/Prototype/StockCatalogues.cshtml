                        <span ng-show="sortType == 'ValueDouble' && !sortReverse" class="fa fa-caret-down"></span>
                        <span ng-show="sortType == 'ValueDouble' && sortReverse" class="fa fa-caret-up"></span>
                    </th>
                    <!-- #endregion -->
                    <!-- #region Nr of Shares -->
